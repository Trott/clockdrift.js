{
  "name": "clockdrift",
  "description": "Use HTTP headers to see if servers have synchronized clocks or not",
  "version": "3.0.0",
  "homepage": "https://github.com/Trott/clockdrift.js",
  "author": {
    "name": "Rich Trott",
    "email": "rtrott@gmail.com"
  },
  "scripts": {
    "test": "standard && c8 --check-coverage --lines 91 --functions 100 --branches 80 node test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Trott/clockdrift.js.git"
  },
  "bugs": {
    "url": "https://github.com/Trott/clockdrift.js/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/Trott/clockdrift.js/blob/master/LICENSE"
    }
  ],
  "main": "clockdrift.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [],
  "preferGlobal": true,
  "bin": {
    "clockdrift": "clockdrift.js"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "c8": "^7.3.5",
    "semantic-release": "^17.2.1",
    "standard": "^15.0.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
